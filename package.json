{
  "name": "meditation-app",
  "version": "0.2.0",
  "description": "Track your meditation time",
  "repository": "https://github.com/alex-bartleynees/react-meditation-app",
  "main": "server/index.js",
  "author": "EDA",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./client/webpack.config.js",
    "dev:client": "npm run build -- --watch",
    "dev:server": "node server",
    "dev": "run-p dev:client dev:server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint . --fix",
    "start": "node server",
    "test": "jest --watchAll --verbose"
    
  },
  "keywords": [
    "fullstack"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "knex": "^0.21.19",
    "lodash": "^4.17.21",
    "pg": "^8.6.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-config-eda": "0.1.4",
    "file-loader": "^6.2.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "superagent": "^5.3.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "15.x",
    "npm": "7.x"
  }
}
